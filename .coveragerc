[run]
source_pkgs =
    pycroft
    ldap_sync
    hades_logs
    web
    tests
omit =
    **/alembic/**
    .pycharm_helpers

[report]
omit =
    **/alembic/**
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod
    if (.*)TYPE_CHECKING:
    @(t(yping)?\.)?overload
